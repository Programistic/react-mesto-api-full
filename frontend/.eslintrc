{
  "parser": "@babel/eslint-parser",
  "settings": {
      "react": {
          "version": "detect"
      }
  },
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "plugin:testing-library/react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
        "jsx": true
    },
    "babelOptions": {
      "presets": ["@babel/preset-react"]
    },
    "ecmaVersion": 11,
    "requireConfigFile": false,
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": 0,
    "react/prop-types": 0
  }
}
